
select 
        `stx_produk`.`produk_id` AS `produk_id`,
        `stx_produk`.`produk_nama` AS `produk_nama`,
        `stx_produk`.`produk_masterbox` AS `produk_masterbox`,
        `stx_produk`.`produk_mesin` AS `produk_mesin`,
        
        `trx_stfg`.`stfg_shift` AS `stfg_shift`,
        `trx_stfg`.`stfg_total` AS `stfg_total`,
        `trx_stfg`.`stfg_tgllaporan` AS `stfg_tgllaporan`,
        
        `trx_master`.`master_id` AS `master_id`,
        `trx_master`.`master_acuan_id` AS `master_acuan_id`,
        `trx_master`.`master_produk_id` AS `master_produk_id`,
        `trx_master`.`master_line` AS `master_line`,
        `trx_master`.`master_tgllaporan` AS `master_tgllaporan`,
        `trx_master`.`master_shift` AS `master_shift`,
        `trx_master`.`master_jumlahteam` AS `master_jumlahteam`,
        `trx_master`.`master_display` AS `master_display`,
        `trx_master`.`master_totalkerjamenit` AS `master_totalkerjamenit`,
        `trx_master`.`master_bayarstfg` AS `master_bayarstfg`,
        `trx_master`.`master_acuanmesin` AS `master_acuanmesin`,
        `trx_master`.`master_acuanline` AS `master_acuanline`
        from ((stx_produk
        join trx_master )
        join trx_stfg )
        where (( `stx_produk`.`produk_id` =  `trx_master`.`master_produk_id` )
        AND ( `trx_master`.`master_shift` = `trx_stfg`.`stfg_shift` )
        And ( `trx_master`.`master_tgllaporan`= `trx_stfg`.`stfg_tgllaporan`) )
        
        
